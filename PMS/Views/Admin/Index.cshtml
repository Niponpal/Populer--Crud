@model IEnumerable<PMS.ModelVm.AdminVm>
@{

}



<a asp-action="CreateOrEdit" asp-controller="Admin" class="btn btn-dark mt-3" type="sumit">Input</a>

<table class="table table-bordered table-responsive" id="Admintable">
    <thead>
        <tr>
     <th>Id</th>
    <th>UserName</th>
    <th>Email</th>
    <th>Password</th>
    <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var  itam in Model)
        {
            <tr>
                <td>@itam.Id</td>
                <td>@itam.UserName</td>
                <td>@itam.Email</td>
                <td>@itam.Password</td>
                <td>
                    <a class="btn btn-primary" asp-action="CreateOrEdit" asp-controller="Admin" asp-route-id="@itam.Id">Edit</a>
                    <a class="btn btn-danger" asp-action="Delted" asp-controller="Admin" onclick="confirm('are you sure')" asp-route-id="@itam.Id">Delted</a>
                    <a class="btn btn-primary" asp-action="Details" asp-controller="Admin" asp-route-id="@itam.Id">Details</a>
                </td>
            </tr>
        }
    </tbody>
</table>


@section Scripts {
    <script>
        new DataTable('#Admintable');
    </script>
}
